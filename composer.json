{
  "name": "hugosoltys/fitz-bundle",
  "type": "symfony-bundle",
  "description": "Browse and install all the bundles you need in your Symfony application in a few seconds.",
  "keywords": ["symfony", "install", "bundle", "fitz"],
  "homepage": "https://github.com/hugosoltys/fitz-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Hugo Soltys",
      "email": "hugo.soltys@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "^3.4 || ^4.0",
    "symfony/templating": "^3.4 || ^4.0",
    "symfony/yaml": "^3.4 || ^4.0",
    "symfony/twig-bundle": "^3.4 || ^4.0",
    "symfony/flex": "^1.4",
    "symfony/process": "^3.4 || ^4.0",
    "symfony/console": "^3.4 || ^4.0",
    "symfony/dotenv": "^3.4 || ^4.0"
  },
  "autoload": {
    "psr-4": { "HugoSoltys\\FitzBundle\\": "" }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "symfony/phpunit-bridge": "^3.4 || ^4.0"
  }
}
